\begin_layout Enumerate
(15 points) Provide a recursive algorithm for evaluating an algebraic expression
 represented as a binary tree.
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

double binaryNode::Eval(BinaryNode rnode)//start by passing root node of
 binary tree
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

	if(isNumber(rnode.operator))
\end_layout

\begin_layout Plain Layout

		return toNumber(rnode.operator);
\end_layout

\begin_layout Plain Layout

	else if(rnode.operator == "+")
\end_layout

\begin_layout Plain Layout

		return Eval(rnode.leftChild) + Eval(rnode.rightChild);
\end_layout

\begin_layout Plain Layout

	else if(rnode.operator == "-")
\end_layout

\begin_layout Plain Layout

		return Eval(rnode.leftChild) - Eval(rnode.rightChild);
\end_layout

\begin_layout Plain Layout

	else if(rnode.operator == "*")
\end_layout

\begin_layout Plain Layout

		return Eval(rnode.leftChild) * Eval(rnode.rightChild);
\end_layout

\begin_layout Plain Layout

	else if(rnode.operator == "/")
\end_layout

\begin_layout Plain Layout

		return Eval(rnode.leftChild) / Eval(rnode.rightChild);
\end_layout

\begin_layout Plain Layout

	else if(rnode.operator == "^")
\end_layout

\begin_layout Plain Layout

		return pow(Eval(rnode.leftChild), Eval(rnode.rightChild));
\end_layout

\begin_layout Plain Layout

	else
\end_layout

\begin_layout Plain Layout

		throw runtime_error("BAD NODE");
\end_layout

\begin_layout Plain Layout

}
